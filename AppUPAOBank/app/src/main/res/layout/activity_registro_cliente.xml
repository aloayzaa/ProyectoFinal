<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:paddingTop="16dp"
    android:paddingBottom="16dp"
    tools:context="com.example.upao.appupaobank.RegistroCliente">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Paterno:"
        android:id="@+id/textApPaterno"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etApPaterno"
        android:layout_below="@+id/textApPaterno"
        android:layout_alignLeft="@+id/textApPaterno"
        android:layout_alignStart="@+id/textApPaterno"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Materno:"
        android:id="@+id/textApMaterno"
        android:layout_below="@+id/etApPaterno"
        android:layout_alignLeft="@+id/etApPaterno"
        android:layout_alignStart="@+id/etApPaterno"
        android:layout_marginTop="25dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/etApMaterno"
        android:layout_below="@+id/textApMaterno"
        android:layout_alignLeft="@+id/textApMaterno"
        android:layout_alignStart="@+id/textApMaterno" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombres:"
        android:id="@+id/textNombres"
        android:layout_below="@+id/etApMaterno"
        android:layout_alignLeft="@+id/etApMaterno"
        android:layout_alignStart="@+id/etApMaterno"
        android:layout_marginTop="25dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/etNombres"
        android:layout_below="@+id/textNombres"
        android:layout_alignLeft="@+id/textNombres"
        android:layout_alignStart="@+id/textNombres" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dni:"
        android:id="@+id/textDni"
        android:layout_below="@+id/etNombres"
        android:layout_alignLeft="@+id/etNombres"
        android:layout_alignStart="@+id/etNombres"
        android:layout_marginTop="25dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/etDni"
        android:layout_below="@+id/textDni"
        android:layout_alignLeft="@+id/textDni"
        android:layout_alignStart="@+id/textDni" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Correo:"
        android:id="@+id/textCorreo"
        android:layout_below="@+id/etDni"
        android:layout_alignLeft="@+id/etDni"
        android:layout_alignStart="@+id/etDni"
        android:layout_marginTop="25dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/etCorreo"
        android:layout_below="@+id/textCorreo"
        android:layout_alignLeft="@+id/textCorreo"
        android:layout_alignStart="@+id/textCorreo" />
    <Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="ACEPTAR"
    android:id="@+id/btnAceptar"
    android:layout_marginTop="50dp"
    android:layout_marginLeft="90dp"
    android:layout_below="@+id/textCorreo"
    android:onClick="onClickAceptar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RETORNAR"
        android:id="@+id/btnRetornar"
        android:layout_alignTop="@+id/btnAceptar"
        android:layout_marginLeft="200dp"
        android:onClick="onCLickRetornar" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout5"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvResultado"
            android:layout_gravity="right"
            android:textSize="13sp"
            android:textStyle="bold|italic" />
    </LinearLayout>

</RelativeLayout>
